{
  "name": "godzie44/yii2-image-behavior",
  "description": "Behavior that will help you to save the modified copy of the uploaded image and give you easy access to them",
  "keywords": [
    "yii",
    "image",
    "behavior",
    "upload image"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/godzie44/yii2-image-upload-helper-behavior/issues",
    "source": "https://github.com/godzie44/yii2-image-upload-helper-behavior"
  },
  "authors": [
    {
      "name": "Derevtsov Konstantin",
      "email": "godzie@yandex.ru"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yurkinx/yii2-image" : "*",
    "yiisoft/yii2": "2.0.*"

  },
  "autoload": {
    "psr-4": {
      "godzie44\\yii\\behaviors\\image\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "require-dev": {
  }
}
